{
  "name": "unrest",
  "version": "1.0.0",
  "private": true,
  "description": "GraphQL proxy to Unlease REST API",
  "main": "index.js",
  "engines": {
    "node": "~6.9.0"
  },
  "pre-commit": {
    "run": [
      "lint"
    ],
    "silent": true
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "scripts": {
    "lint": "eslint --config config/.eslintrc.js server/",
    "start": "NODE_ENV=development nodemon -w server --exec 'npm run lint && babel-node server'",
    "build": "webpack --config config/webpack.config.server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlease-development/unrest.git"
  },
  "author": "Unlease <hello@unlease.io> (https://www.unlease.io/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/unlease-development/unrest/issues"
  },
  "homepage": "https://github.com/unlease-development/unrest#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "eslint": "^3.8.1",
    "eslint-config-unlease": "^3.0.2",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-react": "^6.4.1",
    "node-notifier": "^4.6.1",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.1.3",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "dataloader": "^1.2.0",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "graphql": "^0.7.2",
    "graphql-relay": "^0.4.3",
    "node-fetch": "^1.6.3"
  }
}
